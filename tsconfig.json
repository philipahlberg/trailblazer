{
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "node",
		"lib": ["ES2020"],
		"declaration": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"importHelpers": true
	}
}